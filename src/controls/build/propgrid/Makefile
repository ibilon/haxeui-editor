	@$(CXX) $(CXXFLAGS) -o $@ -c $<

$(OBJDIR)/odcombo.o: ../../src/propgrid/odcombo.cpp
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o $@ -c $<

$(OBJDIR)/extras.o: ../../src/propgrid/extras.cpp
	-@$(CMD_MKOBJDIR)
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o $@ -c $<

-include $(OBJECTS:%.o=%.d)

